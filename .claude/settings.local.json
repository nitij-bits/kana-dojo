{
  "permissions": {
    "defaultMode": "dontAsk",
    "allow": [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "NotebookEdit",
      "Glob",
      "Grep",
      "TodoWrite"
    ],
    "deny": [
      "Bash(git push --force:*)",
      "Bash(git push -f:*)",
      "Bash(git reset --hard:*)",
      "Bash(git clean -fd:*)",
      "Bash(rm -rf:*)",
      "Bash(rm -r:*)",
      "Bash(del /s:*)",
      "Bash(rmdir /s:*)",
      "Bash(format:*)"
    ]
  }
}
